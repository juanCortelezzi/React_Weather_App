{
  "name": "node_testing",
  "version": "1.0.0",
  "description": "node testing server with react",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run intall-client && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'green,blue' \"node server.js\" \"npm run client\""
  },
  "author": "me",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0"
  }
}
